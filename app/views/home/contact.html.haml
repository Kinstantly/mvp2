- @body_class = 'interior about'
- @content_class = 'contact-page no-upper-hr'
- @page_title = "#{company_name} - Contact us"
- info_email = "#{t('contact.emails.info.value')}@#{t('email_domain')}"
- cache 'contact_page' do
	.contact-wrapper
		%section.contact-section
			%header
				%h1 Ask us anything
				%h2
					Email us at
					= mail_to info_email
			.contact-photo
				= image_tag "contactImg.jpg", alt: "Contact us", itemprop: 'image', border: "0"
				%span.photo-credit
					= link_to '&copy; Slava Basovich'.html_safe, profile_path(1509)
			%span.photo-credit.section-bg
				= link_to '&copy; Slava Basovich'.html_safe, profile_path(1509)
			%p Or send a note (or cookies) to: Kinstantly, Inc., 350 Townsend St. #683, San Francisco, CA 94107
